{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "name": "article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "private": false,
      "targetField": "title"
    },
    "content": {
      "type": "richtext"
    },
    "langue": {
      "type": "enumeration",
      "enum": [
        "KB",
        "FR"
      ],
      "required": true
    },
    "author": {
      "columnName": "author",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
